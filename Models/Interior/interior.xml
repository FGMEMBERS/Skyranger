<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>siege</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/Skyranger/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -0.951 </x-m>
      <y-m> -0.325 </y-m>
      <z-m> -0.167 </z-m>
    </offsets>
  </model>

  <!-- Planches de bord -->
  <model>
    <path>Aircraft/Skyranger/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -1.269 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.071 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>  -4.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

</PropertyList>

